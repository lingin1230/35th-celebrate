{
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "5.0.1",
    "parcel": "^2.3.2"
  },
  "scripts": {
    "start": "parcel src/*.html --public-url /static/dist --no-hmr",
    "build": "parcel build src/*.html --public-url /static/dist",
    "watch": "parcel watch src/*.html --public-url /static/dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "cz"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.3.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
